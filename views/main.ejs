<script>
    function openModal() {
        document.getElementById('bgModal').style.visibility = 'visible';
        document.getElementById('modalCriarAtividade').style.visibility = 'visible';
    }
    function openModalActivity() {
        document.getElementById('bgModal').style.visibility = 'visible';
        document.getElementById('modalVisualizarAtividade').style.visibility = 'visible';
    }
    function closeModal() {
        document.getElementById('bgModal').style.visibility = 'hidden';
